  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "undici": "5.6.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "rimraf": "^4.3.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.23.6",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
